apt-get update
apt-get --assume-yes upgrade 
su -c 'setenforce 0'
apt-get --assume-yes install tsu
apt-get --assume-yes install wget
apt-get --assume-yes install git
apt-get --assume-yes install pearl
tsu -c rm -rf  /storage/emulated/0/MALIKI
tsu -c rm -rf  /storage/emulated/0/MALIKIG
tsu -c rm -rf pubg
tsu -c rm -rf inicial
tsu -c rm -rf conf
tsu -c mkdir -p /storage/emulated/0/MALIKI/backup
tsu -c mkdir -p /storage/emulated/0/MALIKIG/backup
tsu -c --assume-yes rm -rf libUE4.so
tsu -c chmod 777 /data/data/com.termux/files/usr/etc/bash.bashrc
tsu -c chmod 777 /data/data/com.termux/files/usr/etc/motd
tsu -c rm -rf /data/data/com.termux/files/usr/etc/bash.bashrc
tsu -c rm -rf /data/data/com.termux/files/usr/etc/motd
tsu -c wget -c  https://github.com/M4L1K1/pubg/releases/download/libs/libUE4.so
tsu -c wget -c  https://github.com/M4L1K1/pubg/releases/download/libsG/libabase.so
tsu -c wget -c  https://github.com/M4L1K1/pubg/releases/download/libsG/libBugly.so
tsu -c wget -c  https://github.com/M4L1K1/pubg/releases/download/libsG/libtprt.so
tsu -c wget -c  https://github.com/M4L1K1/pubg/releases/download/libsG/libzip.so
tsu -c wget -c  https://github.com/M4L1K1/pubg/releases/download/libsG/libzlib.so
tsu -c wget -c  https://github.com/M4L1K1/pubg/releases/download/libsG/AntiCheat.ini
                
tsu -c git clone https://github.com/M4L1K1/pubg.git
tsu -c chmod 777  pubg
tsu -c chmod 777  pubg/bash.bashrc
tsu -c chmod 777  pubg/motd
tsu -c mv pubg/bash.bashrc /data/data/com.termux/files/usr/etc/
tsu -c mv pubg/motd /data/data/com.termux/files/usr/etc/
tsu -c chmod 2755 libUE4.so 
tsu -c chmod 2755 libabase.so
tsu -c chmod 2755 libBugly.so
tsu -c chmod 2755 libtprt.so
tsu -c chmod 2755 libzip.so
tsu -c chmod 2755 libzlib.so
tsu -c chmod 2755 AntiCheat.ini

tsu -c mv libUE4.so  /storage/emulated/0/MALIKI/
tsu -c mv libabase.so /storage/emulated/0/MALIKIG/
tsu -c mv AntiCheat.ini /storage/emulated/0/MALIKIG/
tsu -c mv libBugly.so /storage/emulated/0/MALIKIG/
tsu -c mv libtprt.so /storage/emulated/0/MALIKIG/
tsu -c mv libzip.so /storage/emulated/0/MALIKIG/
tsu -c mv libzlib.so /storage/emulated/0/MALIKIG/

tsu -c mv pubg/hosts  /storage/emulated/0/MALIKI/
tsu -c mv pubg/libUE4.so  /storage/emulated/0/MALIKIG/
tsu -c mv pubg/inicial $HOME
tsu -c mv pubg/conf $HOME
tsu -c chmod 777 conf
tsu -c chmod 777 inicial
tsu -c chmod a+x inicial
tsu -c chmod a+x conf
