#!/data/data/com.termux/files/usr/bin/bash

echo
Menu(){
clear
echo '----------------------------------------------'
echo 'Atenção para as Opções, Não esquecer de alterar
      as Permições da pasta e Sub Pastas Vesão 1.0.45 '
echo '----------------------------------------------'
echo
echo 'Escolha a Versão do Jogo : '
echo 
echo '1. Global'
echo '2. Korea Shadow'
echo '3. Korea 2° opção Libs'
echo '4. SAIR'
echo
echo -n 'Qual a opção desejada : '
read opcao # read le a variável opcao

# Recebe o valor da opção lida e executa a função correspondente
case $opcao in

1) Um ;; # funcao Um
2) Dois ;; # funcao Dois
3) Três ;;
4) Sair ;;
*) 'Opção desconhecida.' ; echo; Menu;;

esac
}


# funcao exibe 1
Um(){
echo
echo 'Iniciando GLobal'
echo
echo "Script exclusivo Membros Clã Shot "
echo
sleep 3 
echo 'injetando libs' 
echo
sleep 3 
echo "Aguarde trabalhando nas Libs...."
echo "Trocando as permissões"
sleep 2 
echo
am start -n com.tencent.ig/com.epicgames.ue4.SplashActivity
echo
iptables -I INPUT -s 183.3.255.64 intl.cloud.tencent.com -j DROP &>/dev/null
iptables -I INPUT -s 183.3.255.64 intl.mtp.tencent.com -j DROP &>/dev/null
iptables -I INPUT -s 183.3.255.64 mtp@tencent.com -j DROP &>/dev/null
iptables -I INPUT -s 127.0.0.1 localhost -j DROP &>/dev/null
iptables -I INPUT -s 183.3.255.64 localhost -j DROP &>/dev/null
iptables -I INPUT -s 127.1.40.2 localhost -j DROP &>/dev/null
iptables -I INPUT -s 127.0.0.1 localhost.localdomain -j DROP &>/dev/null
iptables -I INPUT -s 0.0.0.0 local -j DROP &>/dev/null
iptables -I INPUT -s 255.255.255.255 broadcasthost -j DROP &>/dev/null
iptables -I INPUT -s ::1 localhost -j DROP &>/dev/null
iptables -I INPUT -s ::1 ip6-localhost -j DROP &>/dev/null
iptables -I INPUT -s ::1 ip6-loopback -j DROP &>/dev/null
iptables -I INPUT -s fe80::1%lo0 localhost -j DROP &>/dev/null
iptables -I INPUT -s ff00::0 ip6-localnet -j DROP &>/dev/null
iptables -I INPUT -s ff00::0 ip6-mcastprefix -j DROP &>/dev/null
iptables -I INPUT -s ff02::1 ip6-allnodes -j DROP &>/dev/null
iptables -I INPUT -s ff02::2 ip6-allrouters -j DROP &>/dev/null
iptables -I INPUT -s ff02::3 ip6-allhosts -j DROP &>/dev/null
iptables -I INPUT -s 183.3.255.64 .net -j DROP &>/dev/null
iptables -I INPUT -s 183.3.255.64 .com -j DROP &>/dev/null
iptables -I INPUT -s 183.3.255.64 .org -j DROP &>/dev/null
echo
echo
echo
echo " Alterando as permissões"
echo " libs Ok"

echo "pakeges Ok "
echo
echo
echo 
echo
clear
sleep 80
